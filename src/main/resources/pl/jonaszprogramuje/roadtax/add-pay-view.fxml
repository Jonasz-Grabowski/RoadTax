<?import javafx.scene.layout.*?>


<VBox prefHeight="400.0"
      prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="pl.jonaszprogramuje.roadtax.addPay.AddPayController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
   </padding>
   <TextField fx:id="houseNumber" promptText="który number domu (od drogi) wpłacił "/>
   <TextField fx:id="payNumber" onAction="#enterPayAction" promptText="ile wpłacił"/>
   <Button mnemonicParsing="false" onAction="#enterPayAction" text="Zatwierdź"/>
</VBox>
